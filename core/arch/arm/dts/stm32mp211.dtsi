			interrupt-controller;
			#interrupt-cells = <2>;
			wakeup-gpios = <0>, <0>, <0>, <0>, <0>, <0>;
			status = "disabled";

			vdda18adc: vdda18adc {
				compatible = "st,stm32mp21-pwr-regu";
				regulator-name = "vdda18adc";
				status = "disabled";
