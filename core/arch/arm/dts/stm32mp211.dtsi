			vddio1: vddio1 {
				compatible = "st,stm32mp21-pwr-regu";
				regulator-name = "vddio1";
				st,iocomp = <8 7>;
				status = "disabled";
			};

			vddio2: vddio2 {
				compatible = "st,stm32mp21-pwr-regu";
				regulator-name = "vddio2";
				st,iocomp = <8 7>;
				status = "disabled";
			};

			vddio3: vddio3 {
				compatible = "st,stm32mp21-pwr-regu";
				regulator-name = "vddio3";
